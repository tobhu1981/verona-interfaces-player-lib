{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Verona Interfaces",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "readme": "README.md",
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": true,
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Class",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "TypeAlias",
    "Interface",
    "Enum"
  ],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Functions",
    "Variables",
    "*"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "jsDocCompatibility": true,
  "commentStyle": "all"
}